<div class="card-deck">
    <% object.forEach(event => { %>
        <div class="card text-white bg-dark mb-3" style="border: 2px solid goldenrod">
            <!-- <img class="card-img-top" src="..." alt="Card image cap"> -->  
            <div class="card-body">
              <h5 class="card-title">Event Title: <%= event.title %></h5>
              <h5 class="cart-title">Price: $<%= event.price %></h5>
              <h5 class="cart-title">Location: <%= event.location %></h5>
              <h5 class="cart-title">Date: <%= event.date %></h5>
              <a href="<%= event.url %>" class="btn btn-primary">Book</a>
              <% if (!logged_in) {%>
                <a href="<%= event.url %>" class="btn btn-primary disabled">Add to List</a>
              <% } else { %>
                <a href="<%= event.url %>" class="btn btn-primary">Add to List</a>
              <% } %>  
            </div>
          </div>
    <% }); %>
  </div>